<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Smg_server</servlet-name>
		<servlet-class>org.smg.server.Smg_serverServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Smg_server</servlet-name>
		<url-pattern>/smg_server</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Test</servlet-name>
		<servlet-class>org.smg.server.TestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>developer</servlet-name>
		<servlet-class>org.smg.server.servlet.DeveloperServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>developer</servlet-name>
		<url-pattern>/developer</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>container_weilun</servlet-name>
		<servlet-class>org.smg.server.servlet.container.MatchOperationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>container_weilun</servlet-name>
		<url-pattern>/matches/*</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
